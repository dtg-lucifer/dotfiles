{
  "workbench.productIconTheme": "fluent-icons",
  "errorLens.statusBarColorsEnabled": true,
  "editor.fontFamily": "\"FiraCode Nerd Font Mono\", \"Space Mono\", \"JetBrains Mono\", monospace",
  "editor.fontLigatures": true,
  "explorer.confirmDelete": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.minimap.autohide": true,
  "workbench.sideBar.location": "right",
  "editor.tabSize": 2,
  "editor.showUnused": true,
  "editor.formatOnPaste": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.cursorStyle": "block",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "explorer.compactFolders": false,
  "vim.smartRelativeLine": true,
  "vsicons.dontShowNewVersionMessage": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "notebook.formatOnSave.enabled": true,
  "tailwind-fold.autoFold": false,
  "go.toolsManagement.autoUpdate": true,
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    },
    "nu": {
      "path": "/home/linuxbrew/.linuxbrew/bin/nu",
      "icon": "terminal-ubuntu"
    }
  },
  // "window.titleBarStyle": "native",
  "C_Cpp.default.compilerPath": "/usr/bin/g++",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "workbench.colorCustomizations": {
    "[Material Theme*]": {
      "activityBarBadge.background": "#84FFFF",
      "activityBar.activeBorder": "#84FFFF",
      "list.activeSelectionForeground": "#84FFFF",
      "list.inactiveSelectionForeground": "#84FFFF",
      "list.highlightForeground": "#84FFFF",
      "scrollbarSlider.activeBackground": "#84FFFF50",
      "editorSuggestWidget.highlightForeground": "#84FFFF",
      "textLink.foreground": "#84FFFF",
      "progressBar.background": "#84FFFF",
      "pickerGroup.foreground": "#84FFFF",
      "tab.activeBorder": "#84FFFF",
      "notificationLink.foreground": "#84FFFF",
      "editorWidget.resizeBorder": "#84FFFF",
      "editorWidget.border": "#84FFFF",
      "settings.modifiedItemIndicator": "#84FFFF",
      "settings.headerForeground": "#84FFFF",
      "panelTitle.activeBorder": "#84FFFF",
      "breadcrumb.activeSelectionForeground": "#84FFFF",
      "menu.selectionForeground": "#84FFFF",
      "menubar.selectionForeground": "#84FFFF",
      "editor.findMatchBorder": "#84FFFF",
      "selection.background": "#84FFFF40",
      "statusBarItem.remoteBackground": "#84FFFF"
    }
  },
  "workbench.colorTheme.accentColor": "Sky",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "cpp": true
  },
  // "git.openRepositoryInParentFolders": "never",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "oneDarkPro.vivid": true,
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "*.unused": {
        "foreground": "#FFFF00", // Yellow color
        "fontStyle": "italic underline"
      }
    }
  },
  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*]": {
      "comments": "#fbe843",
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ],
    },
    "[Aura*]": {
      "comments": "#fbe843",
      "textMateRules": [
        {
          "scope": [
            // "keyword",
            // "storage",
            "entity.name.function",
            "support.function",
            "variable.language",
            "keyword.control.async",
            "keyword.control.await"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "meta.preprocessor"
          ],
          "settings": {
            "foreground": "#FFA500",
            // "fontStyle": "underline"
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[Default Dark Modern]": {
      "textMateRules": [
        {
          "scope": [
            // "keyword",
            // "storage",
            "entity.name.function",
            "support.function",
            "variable.language",
            "keyword.control.async",
            "keyword.control.await"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "meta.preprocessor"
          ],
          "settings": {
            // "foreground": "#FFA500",
            // "fontStyle": "underline"
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[*Bearded*]": {
      "textMateRules": [
        {
          "scope": [
            // "keyword",
            // "storage",
            "entity.name.function",
            "support.function",
            "variable.language",
            "keyword.control.async",
            "keyword.control.await"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "meta.preprocessor"
          ],
          "settings": {
            // "foreground": "#FFA500",
            // "fontStyle": "underline"
            "fontStyle": "bold"
          }
        }
      ]
    }
  },
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
  "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
  "go.inlayHints.assignVariableTypes": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "rust-analyzer.inlayHints.closureCaptureHints.enable": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "vim.enableNeovim": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "rust-analyzer.rustfmt.overrideCommand": [
    "cargo",
    "fmt",
    "--",
    "--config-path=rustfmt.toml"
  ],
  "git.autofetch": true,
  "notebook.editorOptionsCustomizations": {
    "editor.tabSize": 4,
    "editor.indentSize": 4,
    "editor.insertSpaces": true
  },
  "editor.fontVariations": "wght@400;700",
  "todohighlight.keywords": [
    {
      "text": "@TODO",
      "color": "black",
      "backgroundColor": "rgba(255, 255, 0, 1)",
      "isRegex": false,
      "fontWeight": "bold",
      "fontStyle": "italic",
      "isCaseSensitive": false
    },
    {
      "text": "@FIXME",
      "color": "rgb(255, 255, 255)",
      "backgroundColor": "grey",
      "fontStyle": "italic",
      "fontWeight": "bold",
      "isRegex": false,
      "isCaseSensitive": false
    },
    {
      "text": "@NOTE",
      "color": "#00FFFF",
      "backgroundColor": "blue",
      "fontStyle": "italic",
      "fontWeight": "bold",
      "isRegex": false,
      "isCaseSensitive": false
    },
    {
      "text": "@DEMO",
      "color": "black",
      "backgroundColor": "orange",
      "fontStyle": "italic",
      "fontWeight": "bold",
      "isRegex": false,
      "isCaseSensitive": false
    },
    {
      "text": "@INFO",
      "color": "black",
      "backgroundColor": "skyblue",
      "fontStyle": "italic",
      "fontWeight": "bold",
      "isRegex": false,
      "isCaseSensitive": false
    }
  ],
  "github.copilot.nextEditSuggestions.enabled": true,
  "ansible.lightspeed.enabled": false,
  "[css]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "git.confirmSync": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "*.templ": "${capture}_templ.go,${capture}_templ.txt",
    "Cargo.toml": "Cargo.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "[templ]": {
    "editor.defaultFormatter": "a-h.templ"
  },
  "vscode_custom_css.imports": [
    "file:///home/piush/.config/vscode_custom.css",
    "file:///home/piush/.config/vscode_custom.js"
  ],
  "editor.copyWithSyntaxHighlighting": false,
  // "diffEditor.ignoreTrimWhitespace": false,
  "editor.emptySelectionClipboard": false,
  "workbench.editor.enablePreview": false,
  "window.newWindowDimensions": "inherit",
  // "editor.multiCursorModifier": "ctrlCmd",
  "files.trimTrailingWhitespace": true,
  // "diffEditor.renderSideBySide": false,
  "editor.snippetSuggestions": "top",
  "editor.detectIndentation": false,
  // "editor.showFoldingControls": "never",
  "editor.guides.indentation": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "workbench.editor.showTabs": "none",
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  // "editor.guides.indentation": false,
  // Silence the Noise
  "breadcrumbs.enabled": false,
  // "scm.diffDecorations": "none",
  // "editor.hover.delay": 1500,
  "editor.hover.enabled": true,
  // "editor.matchBrackets": "never",
  "workbench.tips.enabled": false,
  // "editor.colorDecorators": false,
  // "git.decorations.enabled": false,
  "workbench.startupEditor": "none",
  "editor.lightbulb.enabled": "off",
  "editor.selectionHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  // "editor.occurrencesHighlight": "off",
  // "problems.decorations.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  // "editor.suggestFontSize": 13,
  // "editor.suggestLineHeight": 30,
  // "terminal.integrated.fontSize": 16,
  // "search.useIgnoreFiles": false,
  "search.exclude": {
    // Hide everything in /vendor, except the "laravel" and "livewire" folder.
    "**/vendor/{[^l],?[^ai]}*": true,
    // Hide everything in /public, except "index.php"
    "**/public/{[^i],?[^n]}*": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/_ide_helper.php": true,
    "**/composer.lock": true,
    "**/package-lock.json": true,
    "storage": true,
    ".phpunit.result.cache": true
  },
  /**
    * Code
    **/
  // Include "-" in word selection.
  // "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "emmet.includeLanguages": {
    "blade": "html",
    "vue-html": "html",
    "vue": "html",
    "react": "html",
    "javascript": "html"
  },
  "files.associations": {
    ".php_cs": "php",
    ".php_cs.dist": "php"
  },
  /**
    * PHP
    **/
  "php.suggest.basic": false,
  /**
    * Prettier
    **/
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[tailwindcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "prettier.requireConfig": true,
  "prettier.useEditorConfig": false,
  "explorer.sortOrder": "type",
  "prettier.tabWidth": 4,
  "editor.wordWrapColumn": 120,
  "files.autoSave": "off",
  "files.autoSaveDelay": 2000,
  "editor.quickSuggestions": {
    "strings": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "editor.linkedEditing": true,
  "diffEditor.wordWrap": "on",
  "notebook.output.wordWrap": true,
  "editor.fontSize": 14,
  "editor.minimap.maxColumn": 250,
  "editor.accessibilitySupport": "off",
  "chat.editor.wordWrap": "on",
  "editor.wordWrap": "wordWrapColumn",
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "tailwindCSS.experimental.configFile": null,
  "editor.fontWeight": "400",
  "editor.padding.top": 4,
  "editor.stickyScroll.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 8,
  "workbench.editor.editorActionsLocation": "titleBar",
  "git.openRepositoryInParentFolders": "never",
  "workbench.statusBar.visible": false,
  "animations.Install-Method": "Apc Customize UI++",
  "rust-analyzer.showSyntaxTree": true,
  "rust-analyzer.restartServerOnConfigChange": true,
  "rust-analyzer.inlayHints.genericParameterHints.const.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "workbench.colorTheme": "Bearded Theme Monokai Black",
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "workbench.activityBar.location": "hidden",
}
